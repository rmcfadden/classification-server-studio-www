  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "strict": true,
    "jsxImportSource": "solid-js",
    "types": ["vite-plugin-solid-svg/types-component-solid","vite/client"],
